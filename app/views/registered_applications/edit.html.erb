<h1>Edit Application</h1>

<br>
  <div>
    <div class="form-group"><%= f.label :title %>
      <%= f.text_field :title, class: "form-control" %>
    </div>
    <div class="form-group"><%= f.label :body %>
      <%= f.text_area :body, class: "form-control" %>
    </div>
    <div class="form-group"><%= f.check_box :private %>
      <%= f.label :private? %>
    </div>
     <div class="submit"><%= f.submit %>
     </div>
</div>
<% end %>